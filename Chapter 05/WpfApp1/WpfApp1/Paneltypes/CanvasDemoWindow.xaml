<Window x:Class="WpfApp1.Paneltypes.CanvasDemoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.Paneltypes"
        mc:Ignorable="d"
        Title="CanvasDemoWindow" Height="450" Width="800">
    <Canvas>
        <Rectangle Width="300" Height="300" Fill="Black"/>
        <Ellipse Width="100" Height="100" Fill="LightGray"/>
        <Ellipse Width="100" Height="100" Canvas.Left="290" Canvas.Top="10" Fill="Black" Panel.ZIndex="1"/>
        <Rectangle Width="100" Height="100" Canvas.Left="290" Canvas.Top="10" Fill="LightGray"/>
    </Canvas>
</Window>
